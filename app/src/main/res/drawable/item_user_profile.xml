<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="oval">
            <gradient
                android:angle="360"
                android:startColor="#2F7F28"
                android:endColor="#00FF38"
                android:type="linear" />
            <size android:width="24dp"
                android:height="24dp"/>
        </shape>

    </item>

    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="oval" >
            <solid android:color="@color/sharehands_gray" />
        </shape>
    </item>

    <item
        android:drawable="@drawable/ic_profile_default"
        android:top="8dp"
        android:bottom="8dp"
        android:start="8dp"
        android:end="8dp"/>





</layer-list>